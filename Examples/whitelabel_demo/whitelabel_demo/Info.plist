	<string>$(MISNAP_LICENSE_KEY)</string>
	<key>UIAppFonts</key>
	<array>
		<string>WorkSans-Black.otf</string>
		<string>WorkSans-Bold.otf</string>
		<string>WorkSans-ExtraBold.otf</string>
		<string>WorkSans-ExtraLight.otf</string>
		<string>WorkSans-Light.otf</string>
		<string>WorkSans-Medium.otf</string>
		<string>WorkSans-Regular.otf</string>
		<string>WorkSans-Thin.otf</string>
